{
  "name": "relay-github",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "npm run relay:build && next dev",
    "build": "npm run relay:build && next build",
    "start": "next start",
    "lint": "next lint",
    "relay:build": "relay-compiler",
    "relay:watch": "relay-compiler --watch"
  },
  "dependencies": {
    "babel-plugin-relay": "^14.0.0",
    "next": "12.2.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-relay": "^14.0.0",
    "recoil": "^0.7.4",
    "relay-runtime": "^14.0.0"
  },
  "devDependencies": {
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-relay": "^13.0.2",
    "@types/relay-runtime": "^13.0.3",
    "eslint": "8.20.0",
    "eslint-config-next": "12.2.3",
    "relay-compiler": "^14.0.0",
    "typescript": "4.7.4"
  },
  "relay": {
    "src": "./src/",
    "schema": "./schema.graphql",
    "language": "typescript",
    "exclude": [
      "**/node_modules/**",
      "**/__mocks__/**",
      "**/__generated__/**"
    ]
  }
}
